<nav>
  <% if (!user) { %>
    <a href="/auth/sign-up">Sign Up</a>
    <a href="/auth/sign-in">Sign In</a>
  <% } else { %>
    <a href="/listings/all">Home Page</a>
    <% if (user.isAdmin) { %>
      <a href="/listings/requests">View Requests</a>
    <% } %>
    <a href="/users/<%= user._id %>">My Profile (<%= user.username %>)</a>
    <a href="/auth/sign-out">Sign Out</a>
  <% } %>
</nav>
